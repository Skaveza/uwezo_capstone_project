{
  "dataset_name": "Kenyan Synthetic Documents",
  "version": "1.0",
  "description": "Synthetic Kenyan National IDs and Bank Statements for document verification training",
  "total_samples": 100,
  "splits": {
    "train": 70,
    "validation": 15,
    "test": 15
  },
  "document_types": [
    "national_id",
    "bank_statement"
  ],
  "fields": {
    "national_id": [
      "document_type",
      "id_number",
      "full_name",
      "date_of_birth",
      "sex",
      "district_of_birth",
      "date_of_issue"
    ],
    "bank_statement": [
      "document_type",
      "bank",
      "account_name",
      "account_number",
      "start_date",
      "end_date",
      "opening_balance",
      "closing_balance",
      "transactions"
    ]
  },
  "quality_levels": [
    "excellent",
    "good",
    "fair",
    "poor"
  ]
}